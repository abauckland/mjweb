<h1>Listing images</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @imagesettings.each do |imagesetting| %>
      <tr>
        <td><%= image_tag @imagesetting.image.photo(:thumb) %></td>
        <td><%= link_to 'Destroy', imagesetting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Add Image', new_imagessetting_path %>

